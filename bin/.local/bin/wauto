#!/bin/sh
# autostart script for wlroots based tiling compositors

wl-paste -t text --watch clipman store --no-persist &
# swaybg -i /home/skovati/docs/pics/pape -m fill &
yambar &
swayidle -w timeout 600 'lock' before-sleep 'lock' lock 'lock' &
gammastep -l 43:-89 -t 5500:3600 &
easyeffects --gapplication-service &
sudo sh -c "echo 0 > /sys/devices/platform/thinkpad_acpi/leds/platform::micmute/brightness"
